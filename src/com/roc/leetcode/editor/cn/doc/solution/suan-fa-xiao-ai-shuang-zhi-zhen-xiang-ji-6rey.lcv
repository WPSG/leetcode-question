```C++
class Solution {
public:
    bool isSubsequence(string s, string t) {
        // åˆ¤æ–­ t æ˜¯å¦æ˜¯ s çš„å­åºåˆ—

        int n = s.size(), m = t.size();
        
        // å¦‚æœ t é•¿åº¦å¤§äº sï¼Œä¸€å®šä¸æ˜¯å­åºåˆ—
        if(m > n) return false;

        // è®°å½•å½“å‰ s ä¸­åŒ¹é…åˆ°äº†å“ªä¸ªä½ç½®
        int i = 0;
        for(char ch : t) {
            while(i < n && s[i] != ch) i++;
            if(i >= n) return false;

            // æ­¤æ—¶ s[i] = chï¼Œä¸‹æ¬¡è¦ä» s[i + 1] å¼€å§‹åŒ¹é…
            i++;
        }

        return true;
    }

    string findLongestWord(string s, vector<string>& dictionary) {
        // æ›´é•¿çš„ã€å­—å…¸åºæ›´å°çš„æ’åœ¨å‰é¢ï¼Œè¿™æ ·ä¸€æ—¦æ‰¾åˆ°ï¼Œå°±æ˜¯ç»“æœ
        sort(dictionary.begin(), dictionary.end(), [&](string &a, string &b) {
            if(a.size() == b.size()) return a < b;
            return a.size() > b.size();
        });

        for(string &t : dictionary) {
            if(isSubsequence(s, t)) return t;
        }

        // å¦‚æœæ²¡æ‰¾åˆ°
        return "";
    }
};
```

