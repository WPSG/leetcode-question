class Solution {
    private boolean isSubsequence(char[] s, char[] t) {
        // åˆ¤æ–­ t æ˜¯å¦ä¸º s çš„å­åºåˆ—

        int n = s.length, m = t.length;

        // å¦‚æœ t é•¿åº¦å¤§äº sï¼Œä¸€å®šä¸æ˜¯å­åºåˆ—
        if(m > n) return false;

        // è®°å½•å½“å‰ s ä¸­åŒ¹é…åˆ°äº†å“ªä¸ªä½ç½®
        int i = 0;

        for(char ch : t) {
            while(i < n && s[i] != ch) i++;
            if(i >= n) return false;

            // æ­¤æ—¶ s[i] = chï¼Œä¸‹æ¬¡è¦ä» s[i + 1] å¼€å§‹åŒ¹é…
            i++;
        }

        return true;
    }

    public String findLongestWord(String s, List<String> dictionary) {
        var arr = s.toCharArray();

        // ç¬¬ä¸€ä¼˜å…ˆçº§é•¿åº¦ï¼Œè¶Šé•¿çš„è¶Šé å‰ï¼›ç¬¬äºŒä¼˜å…ˆçº§å­—å…¸åºï¼Œè¶Šå°çš„è¶Šé å‰
        dictionary.sort((a, b) -> {
            if (a.length() != b.length()) {
                return Integer.compare(b.length(), a.length());
            }
            return a.compareTo(b);
        });

        for (var str : dictionary) {
            if (isSubsequence(arr, str.toCharArray())) {
                return str;
            }
        }

        // æœªæ‰¾åˆ°ï¼Œè¿”å›ç©ºå­—ç¬¦ä¸²
        return "";
    }
}
