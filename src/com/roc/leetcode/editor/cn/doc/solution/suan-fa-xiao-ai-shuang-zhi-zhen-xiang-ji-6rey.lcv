class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        n = len(s)
        m = len(t)
        if m > n:
            return False

        i = 0
        for ch in t:
            while i < n and s[i] != ch:
                i += 1
            if i >= n:
                return False

            # æ­¤æ—¶ s[i] = chï¼Œä¸‹æ¬¡è¦ä» s[i + 1] å¼€å§‹åŒ¹é…
            i += 1
        return True

    def findLongestWord(self, s: str, dictionary: List[str]) -> str:
        # lambda æ’åºï¼šé•¿åº¦è¶Šå¤§çš„æ’åœ¨å‰é¢ï¼Œå­—å…¸åºè¶Šå°çš„æ’åœ¨å‰é¢
        dictionary.sort(key = lambda x: (-len(x), x))

        for word in dictionary:
            if self.isSubsequence(s, word):
                return word
            
        return ''
